{
    "name": "krproxy/excurso",
    "type": "library",
    "description": "система организации екскурсий",
    "keywords": [
        "krproxy",
        "excurso"
    ],
    "homepage": "https://github.com/krproxy/excurso",
    "license": "MIT",
    "authors": [
        {
            "name": "totorro",
            "email": "krproxy@gmail.com",
            "homepage": "#",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "kalnoy/nestedset": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "krproxy\\excurso\\": "src",
            "krproxy\\excurso\\\\Http\\Controllers\\": "src/Http/Controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "krproxy\\excurso\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
